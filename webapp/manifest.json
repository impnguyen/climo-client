{
	"_version": "1.7.0",
	"sap.app": {
		"id": "mpn.climo.client",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"climoSrv": {
				"uri": "http://127.0.0.1:8000/sap/opu/odata/sap/Z_C_CLIMO_ERROR_CDS/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/Z_C_CLIMO_ERROR_CDS/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://sys-monitor",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "mpn.climo.client.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"componentName": "climoClientComponent",
		"dependencies": {
			"minUI5Version": "1.52.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			},
			"components": {

			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "mpn.climo.client.i18n.i18n"
				},
				"preload": true
			},
			"": {
				"dataSource": "climoSrv",
				"settings": {
					"loadAnnotationsJoined": true,
					"useBatch": true,
					"defaultBindingMode": "TwoWay"
				},
				"preload": false
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"clearTarget": false,
				"viewPath": "mpn.climo.client.view",
				"controlId": "climoClient",
				"controlAggregation": "pages",
				"homeRoute": "log",
				"transition": "flip",
				"async": true
			},
			"routes": [
			],
			"targets": {
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/namespace/appname/WebContent",
		"_version": "1.1.0"
	}
}
